<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisplus.model.gie.mapper.UserMapper">

    <!-- 通用实体类映射 -->
    <resultMap type="com.example.mybatisplus.model.gie.entity.User" id="BaseResultMap">
        <result column="user_id" property="userId"/>
        <result column="account" property="account"/>
        <result column="password" property="password"/>
        <result column="salt" property="salt"/>
        <result column="pd" property="pd"/>
        <result column="idCard" property="idCard"/>
    </resultMap>

    <!-- 通用查询字段列 -->
    <sql id="Base_Column_List">
        user_id, account, password, salt, pd, idCard
    </sql>

</mapper>
